    "react-router-dom": "5.2.0",
    "react-test-renderer": "16.13.1",
    "react-with-direction": "1.3.1",
    "redux": "4.0.5",
    "redux-mock-store": "1.5.4",
    "redux-thunk": "2.3.0",
    "uuid": "8.1.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-merge": "5.1.1"
