{
  "name": "movie-night",
  "sideEffects": [
    "*.css",
    "*.scss",
    "./server/utils/env.js"
  ],
  "version": "1.0.0",
  "main": "index.js",
  "author": "Pawel Grimm",
  "license": "MIT",
  "scripts": {
    "build:client": "webpack --config webpack.prod.js",
    "build:prod": "webpack -p --env production",
    "build:server": "rimraf ./server/dist && babel ./server -d ./server/dist",
    "babel-node": "babel-node ./server/server.js",
    "dev": "concurrently \"yarn run dev:front\" \"yarn run dev:back\"",
    "dev:front": "webpack-dev-server --config webpack.dev.js",
    "dev:back": "nodemon --exec yarn run babel-node",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.json",
    "start": "node server/dist/server.js",
    "build": "yarn run build:client && yarn run build:server",
    "test:pre-commit": "yarn run test -o -b"
  },
  "pre-commit": [
    "test:pre-commit"
  ],
  "dependencies": {
    "@babel/node": "7.10.5",
    "@babel/plugin-proposal-optional-chaining": "7.10.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "app-root-path": "3.0.0",
    "axios": "0.19.2",
    "classnames": "2.2.6",
    "concurrently": "5.2.0",
    "core-js": "3.6.5",
    "cross-env": "7.0.2",
    "date-fns": "2.15.0",
    "dompurify": "2.0.12",
    "dotenv": "8.2.0",
    "dotenv-cli": "3.2.0",
    "express": "4.17.1",
    "express-validator": "6.6.0",
    "firebase": "7.15.0",
    "history": "4.10.1",
    "jsdom": "16.3.0",
    "moment": "2.26.0",
    "moviedb-promise": "3.0.3",
    "nodemon": "2.0.4",
    "normalize.css": "8.0.1",
    "numeral": "2.0.6",
    "pre-commit": "1.2.2",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-feather": "2.0.8",
    "react-image-fallback": "8.0.0",
    "react-loading": "2.0.3",
    "react-modal": "3.11.2",
    "react-outside-click-handler": "1.3.0",
    "react-player": "2.3.1",
